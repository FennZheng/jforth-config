<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<!--<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/styles/**</from>
		<to>/styles/$1</to>
	</rule>-->
    <rule match-type="wildcard">
		<from>/springapp/**</from>
		<to>/springapp/$1</to>
	</rule>
	<rule>
		<from>^/([^\.]*|.*/[^\.]+|.*/)$</from><!-- / or /abc or /ab.c/def or /ab.c/ -->
		<to>/springapp/$1</to>
	</rule>
	<outbound-rule match-type="wildcard">
		<from>/springapp/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>