<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/aop
	http://www.springframework.org/schema/aop/spring-aop-3.0.xsd ">
    <aop:aspectj-autoproxy />
    <bean id="dynamicPropertyAspect" class="org.xforth.config.aop.DynamicPropertyAspect"/>
    <bean id="configBundle" class="org.xforth.config.client.ConfigBundle">
        <constructor-arg name="schema" value="${config.project}"/>
        <constructor-arg name="configFile" value="${config.file}"/>
        <constructor-arg name="zkConnectString" value="${config.zookeeper}"/>
        <constructor-arg name="isDynamic" value="${config.support.dynamic}"/>
    </bean>
</beans>